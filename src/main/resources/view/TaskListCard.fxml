<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox id="cardPane" fx:id="cardPane" minWidth="280.0" prefHeight="114.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100" />
            </columnConstraints>
            <children>
                <VBox alignment="CENTER_LEFT" maxHeight="150.0" minHeight="105.0" minWidth="180.0" prefHeight="115.0" GridPane.columnIndex="0">
                    <stylesheets>
                        <URL value="@DarkTheme.css" />
                        <URL value="@Extensions.css" />
                    </stylesheets>
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>

                    <children>
                        <HBox alignment="CENTER_LEFT" spacing="5">
                            <children>
                                <HBox>
                                    <Label fx:id="id" styleClass="cell_big_label" />
                                </HBox>
                                 <Label fx:id="name" styleClass="cell_big_label" text="\$first" />
                            </children>
                        </HBox>
                  <HBox>
                     <children>
                                <Label fx:id="tags" styleClass="cell_small_label" text="\$tags" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label fx:id="startAtLabel" layoutX="10.0" layoutY="10.0" styleClass="cell_small_label" text="\$startAtLabel" />
                              <Label fx:id="startDate" styleClass="cell_small_label" text="\$startDate" />
                              
                                                      <Label fx:id="startTime" styleClass="cell_small_label" text="\$startTime">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label fx:id="endAtLabel" layoutX="10.0" layoutY="10.0" styleClass="cell_small_label" text="\$endAtLabel" />
                        <Label fx:id="endDate" styleClass="cell_small_label" text="\$endDate" />
                              <Label fx:id="endTime" styleClass="cell_small_label" text="\$endTime">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin></Label>
                     </children>
                  </HBox>
                    </children>
                </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <HBox>
                     <children>
                        <Label fx:id="isComplete" prefHeight="15.0" prefWidth="118.0" styleClass="cell_personCard_complete" text="\$isComplete" textAlignment="CENTER">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <padding>
                  <Insets left="55.0" />
               </padding>
            </VBox>
            </children>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
</HBox>
